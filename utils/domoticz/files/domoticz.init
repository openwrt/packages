START=81

start() {
	service_start /opt/domoticz/domoticz -www 8080 -wwwroot /opt/domoticz/www -dbase /opt/domoticz/domoticz.db -startupdelay 5 -daemon
}

stop() {
	service_stop /opt/domoticz/domoticz
}
