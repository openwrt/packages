+					/* we put 0000 into our buffer to represent both Warm and Cold white */
+					ssbuf << "0000";
+					usingbuf = true;
+				}
+				if ((f_capabilities) & (1 << (COLORIDX_AMBER)))
+				{
+					/* if AMBER is present, append our buffer if needed */
