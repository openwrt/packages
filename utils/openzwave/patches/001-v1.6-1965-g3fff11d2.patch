+			<Item label="Bright" value="0" />
+			<Item label="Medium" value="1" />
+			<Item label="Low" value="2" />
+		</Value>
+		<Value type="int" genre="config" index="3" label="Auto Turn-Off Timer" size="4" min="0" max="65535" value="0" units="minutes">
