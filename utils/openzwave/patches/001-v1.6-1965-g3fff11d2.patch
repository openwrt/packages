+			<Item label="Custom Level" value="1" />
+			<Item label="Maximum Level" value="2" />
+			<Item label="Full" value="3" />
+		</Value>
+		<Value type="list" genre="config" index="13" label="Enable/Disable Scene Control" size="1" min="0" max="1" value="0">
