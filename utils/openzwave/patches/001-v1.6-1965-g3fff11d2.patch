+				else
+					ss << "00";
+				/* do B */
+				if ((f_capabilities) & (1 << (COLORIDX_BLUE)))
+					ss << std::setw(2) << std::uppercase << std::hex << std::setfill('0') << (int) m_colorvalues[COLORIDX_BLUE];
+				else
+					ss << "00";
