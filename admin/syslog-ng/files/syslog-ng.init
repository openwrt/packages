#!/bin/sh /etc/rc.common
# Copyright (C) 2006-2019 OpenWrt.org

START=12

USE_PROCD=1

start_service() {
	[ -f /etc/syslog-ng.conf ] || return 1
	procd_open_instance
	procd_set_param command /usr/sbin/syslog-ng --foreground
	procd_close_instance
}

reload_service() {
	/usr/sbin/syslog-ng-ctl reload
}
