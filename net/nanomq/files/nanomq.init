START=95
STOP=10

USE_PROCD=1
PROG=/usr/bin/nanomq
CONFIGFILE=/etc/nanomq.conf

start_service() {
	procd_open_instance
	procd_set_param command "$PROG" start --conf "$CONFIGFILE"
	procd_close_instance
}
