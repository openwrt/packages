-        cimg::X11attr().nb_wins--;
-
-        // Destroy window, image, colormap and title
-        if (is_fullscreen) _desinit_fullscreen();
-        XDestroyWindow(cimg::X11attr().display,window);
-        window = 0;
-#ifdef cimg_use_xshm
-        if (shminfo) {
-          XShmDetach(cimg::X11attr().display, shminfo);
