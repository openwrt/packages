-  const double quad_d2zdxdy10i = uno_two - uno_fou + dble_dzdx11i;
-  const double quad_d2zdxdy01i = qua_thr - qua_one - dble_dzdx00i;
-  const double quad_d2zdxdy11i = qua_fou - qua_two - dble_dzdx10i;
