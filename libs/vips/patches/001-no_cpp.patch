-        }
-      } break;
-      case ButtonPress: {
-        do {
-          switch (event.xbutton.button) {
-          case 1: std::memmove((void*)(buttons+1),(void*)buttons,255); button|=1; is_event = true; break;
-          case 2: std::memmove((void*)(buttons+1),(void*)buttons,255); button|=4; is_event = true; break;
