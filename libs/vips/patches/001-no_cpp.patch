-        }
-      } break;
-      default: {
-        for (T* ptr = buffer+size; ptr>buffer; --ptr) {
-          unsigned char *pb=(unsigned char*)(--ptr), *pe=pb+sizeof(T);
-          for (int i=0; i<(int)sizeof(T)/2; i++) cimg::swap(*(pb++),*(--pe));
-        } break;
-      }
-      }
