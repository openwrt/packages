-static void
-args_free (Args *args)
-{
-  int i;
-
-  for (i = 0; i < args->n; i++)
-    IM_FREE (args->str[i]);
-  args->n = 0;
-  args->argc = 0;
-  IM_FREE (args->str);
-  IM_FREE (args->argv);
-  IM_FREE (args);
-}
