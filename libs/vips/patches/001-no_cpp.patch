-            if (sizey>vec[1]) vec[1] = sizey;
-            vec[2] = sn;
-          }
-          st_slices[st_slices.size-1][7] = (float)i;
-        }
-      } while (err==7);
-
-      // Read data
-      std::FILE *file2 = cimg::fopen(filenamerec,"rb");
-      CImgList<T> dest;
-      { cimglist_for(st_global,l) {
-        const CImg<unsigned int>& vec = st_global[l];
