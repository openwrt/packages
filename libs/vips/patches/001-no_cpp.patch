-  const double dble_slopelimit_10 = 6.0 * NOHALO_MIN( u10, v10 );
-  const double dble_slopelimit_01 = 6.0 * NOHALO_MIN( u01, v01 );
-  const double dble_slopelimit_11 = 6.0 * NOHALO_MIN( u11, v11 );
