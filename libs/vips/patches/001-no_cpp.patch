-      float X=0,Y=0,Z=0;
-      cimg_forX(points,l) {
-        if ((err = std::fscanf(file,"%f%f%f%*[^\n]",&X,&Y,&Z))!=3) {
-          cimg::fclose(file);
-          throw CImgIOException("CImg<%s>::get_load_off() : File '%s', cannot read point %u.\n",pixel_type(),filename,l);
-        }
