-        // Reset display variables
-        if (title) delete[] title;
-        width = height = normalization = events = 0;
-        is_fullscreen = is_resized = is_moved = is_event = false;
-        is_closed = true;
-        title = 0;
-        window_x = window_y = window_width = window_height = mouse_x = mouse_y = wheel = 0;
