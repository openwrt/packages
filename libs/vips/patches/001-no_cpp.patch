-  const double sign_dzdy00 = NOHALO_SIGN( dble_dzdy00i );
-  const double sign_dzdy10 = NOHALO_SIGN( dble_dzdy10i );
-  const double sign_dzdy01 = NOHALO_SIGN( dble_dzdy01i );
-  const double sign_dzdy11 = NOHALO_SIGN( dble_dzdy11i );
